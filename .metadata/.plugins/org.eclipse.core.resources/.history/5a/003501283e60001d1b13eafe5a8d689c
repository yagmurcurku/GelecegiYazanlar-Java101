
public class Driver {

	public static void main(String[] args) {
		
		int number = 10;
		
		boolean result = number > 0;
		
		if(result) {
			System.out.println(number + " pozitif bir sayıdır");
			System.out.println("2.");
		}
			
		
		
		System.out.println("Burası her zaman çalıştırılacak.");
		
		

	}

}
