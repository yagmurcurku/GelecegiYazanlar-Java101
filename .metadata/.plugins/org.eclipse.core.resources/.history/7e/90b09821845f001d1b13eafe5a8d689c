import java.util.Arrays;

public class Driver {

	public static void main(String[] args) {
		int[] numbers;
		numbers = new int[16];
		System.out.println(Arrays.toString(numbers));

		numbers[5] = 32;
		System.out.println(numbers[5]);

		int[] numbers2 = {1,2,3,4,5,6,7,8,9};
		System.out.println(numbers[5]);
		
	}

}
