import java.util.Arrays;

public class Driver {

	public static void main(String[] args) {
		int[] numbers = new int[5];
		numbers[0] = 5;
		numbers[1] = 10;
		numbers[2] = 20;
		numbers[3] = 30;
		numbers[4] = 40;
		
		System.out.println(numbers.length);
		System.out.println(Arrays.toString(numbers));
		
		System.out.println("-------------------------");
		
		for ( int i = 0; i<5; i++ ) {
			System.out.print(numbers[i]);
		}
	}

}
