
public class Driver {

	public static void main(String[] args) {
		
		int x = 10;
		int y = 20;
		
		float y2 = 20f;
		
		System.out.println(x + y);
		System.out.println(x - y);
		System.out.println(x * y);
		System.out.println(x / y);
		System.out.println(x / y2);

		System.out.println(5 % 2);


	}

}
