
public class Driver {

	public static void main(String[] args) {
		
		boolean b = false;
		
		System.out.println(b);
		System.out.println(!b);

		System.out.println(b & false);
		
		//& operatörü
		//false & false = false
		//false & true = false
		//true & false = false
		//true & true = true
		
		//| operatörü
		//false | false = false
		//false | true = true
		//true | false = true
		//true | true = true
	
		System.out.println(b | false);
	}

}
